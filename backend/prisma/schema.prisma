generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

// Definição do modelo Todo
model Todo {
  id    Int @id @default(autoincrement()) // Define o campo 'id' como identificador único
  text        String    // Título da tarefa
  completed   Boolean   // Indica se a tarefa está completa ou não
}